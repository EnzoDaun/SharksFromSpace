{
  "version": 2,
  "builds": [
    {
      "src": "serverless.ts",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/serverless.ts"
    }
  ],
  "functions": {
    "serverless.ts": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}